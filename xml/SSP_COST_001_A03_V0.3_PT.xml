<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Visitas de seguimento de detecção activa de casos_2016 V0.3_PT</h:title>
    <model>
      <instance>
        <SSP_COST_001_A03_V0.3_PT id="SSP_COST_001_A03_V0.3_PT">
          <agg_info>
            <family_id/>
          </agg_info>
          <socio_demo>
            <birth_date/>
            <gender/>
            <visit_date/>
            <consent/>

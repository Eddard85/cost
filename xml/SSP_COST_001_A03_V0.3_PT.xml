      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/perm_id" type="string"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/child_malaria" required="true()" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/malaria_cost" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/child_malaria  = 1"/>

      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/amostras/positive_test" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/amostras/test_result  = 1" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/amostras/why_not" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/amostras/positive_test  = 2" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/amostras/speci" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/amostras/why_not  = 3" type="string"/>

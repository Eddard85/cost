      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/socio_demo/consent" required="true()" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/socio_demo/consent_why" relevant=" /SSP_COST_001_A03_V0.3_PT/socio_demo/consent  = 1" required="true()" type="string"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos" relevant=" /SSP_COST_001_A03_V0.3_PT/socio_demo/consent  = 2"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/malaria_control/rebouco" required="true()" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/malaria_control/net_sleepun" required="true()" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/malaria_control/net_furos" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/malaria_control/net_sleepun  = 1" required="true()" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/malaria_control/net_loc" required="true()" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/malaria_control/net_wash" required="true()" type="select1"/>

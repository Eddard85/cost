      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/clinic_data/fever_detec" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/clinic_data/anti_malarians" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/clinic_data/drugs_type" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/clinic_data/anti_malarians  = 1" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/clinic_data/drugs_specifiy" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/clinic_data/drugs_type  = 7" type="string"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/clinic_data/vacins" type="select1"/>

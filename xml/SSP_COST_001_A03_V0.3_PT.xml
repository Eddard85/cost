      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/traditional_med" required="true()" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/trad_assist" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/traditional_med  = 1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/trad_assist/con_cost" required="true()" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/trad_assist/trad_visitno" required="true()" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/trad_assist/material_cost" required="true()" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/trad_assist/travel_cost" required="true()" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/responsbl_assist" required="true()" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/respo_assist" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/responsbl_assist  = 1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/respo_assist/cons_cost" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/respo_assist/test_cost" type="int"/>

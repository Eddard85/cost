          <meta>
            <instanceID/>
            <instanceName/>
          </meta>
        </SSP_COST_001_A03_V0.3_PT>
      </instance>
      <bind constraint="(regex(., '^[0-9][0-9][0-9]-[0-9][0-9][0-9][0-9]-[0-9][0-9][0-9]$'))" jr:constraintMsg="Formato XXX-XXXX-XXX" nodeset="/SSP_COST_001_A03_V0.3_PT/agg_info/family_id" required="true()" type="string"/>
      <bind constraint="(regex(.,'^[12]{1}[0-9]{3}-0[1-9]{1}-[0-2]{1}[0-9]{1}$')) or (regex(.,'^[12]{1}[0-9]{3}-0[1-9]{1}-3[0-1]{1}$')) or (regex(.,'^[12]{1}[0-9]{3}-1[0-2]{1}-3[0-1]{1}$')) or (regex(.,'^[12]{1}[0-9]{3}-1[0-2]{1}-[0-2]{1}[0-9]{1}$'))" nodeset="/SSP_COST_001_A03_V0.3_PT/socio_demo/birth_date" required="true()" type="date"/>

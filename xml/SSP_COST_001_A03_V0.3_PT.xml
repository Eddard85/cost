      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/intern/days_no" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/intern/call_cost" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/intern/feeding_cost" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/intern/despesa" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/intern/espec_despesa" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/intern/despesa  = 1" type="string"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/intern/desp_cost" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/intern/despesa  = 1" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/cost_after_intern" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities_cancel" type="select1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities" relevant=" /SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities_cancel  = 1"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities/agricul" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities/indus" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities/comer" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities/serv" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities/adminis" type="int"/>

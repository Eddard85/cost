      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities/hosp_work" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities/domestic_work" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities/study" type="int"/>
      <bind nodeset="/SSP_COST_001_A03_V0.3_PT/seguimentos/child_repeat/child/preencher/activities/rdperson_payment" type="select1"/>
